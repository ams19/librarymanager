<AnchorPane prefHeight="600" prefWidth="500" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <TextField fx:id="username" layoutX="147.0" layoutY="341.0" prefHeight="41.0" prefWidth="209.0" promptText="Username">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <PasswordField fx:id="pwd" layoutX="147.0" layoutY="397.0" prefHeight="41.0" prefWidth="209.0" promptText="Password">
         <font>
            <Font size="16.0" />
         </font>
      </PasswordField>
      <Label layoutX="149.0" layoutY="300.0" prefHeight="41.0" prefWidth="204.0" text="     Enter Login Details">
         <font>
            <Font size="16.0" />
         </font></Label>
      <ImageView disable="true" fitHeight="223.0" fitWidth="249.0" layoutX="149.0" layoutY="64.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images.jpeg" />
         </image>
      </ImageView>
      <Label layoutX="13.0" layoutY="-11.0" prefHeight="75.0" prefWidth="466.0" text="      NATIONAL INSTITUTE OF TECHNOLOGY, JALANDHAR" textFill="#1a0b0b">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button layoutX="198.0" layoutY="495.0" mnemonicParsing="false" onAction="#clicklogin" prefHeight="33.0" prefWidth="85.0" text="Login">
