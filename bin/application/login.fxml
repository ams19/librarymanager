      </Button>
      <Label layoutX="231.0" layoutY="536.0" text="OR" />
      <Button layoutX="198.0" layoutY="560.0" mnemonicParsing="false" onAction="#clicknew" prefHeight="26.0" prefWidth="85.0" text="New User" />
      <ComboBox fx:id="combo_box" layoutX="149.0" layoutY="453.0" prefHeight="26.0" prefWidth="209.0" promptText="Select" />
   </children>
</AnchorPane>
